{# MOTU assignment query results template #}
{% trans_default_domain 'queries' %}
<div class="table-responsive">
  <table
    id="result-table"
    class="table table-condensed table-hover table-striped">
    <thead>
      <tr>
        <th id="col-taxname">
          {{ "columns.taxon" | trans}}
        </th>
        <th>{{"columns.method" | trans}}</th>
        <th>{{"columns.dataset" | trans}}</th>
        <th>{{"columns.seqs" | trans}}</th>
        <th>{{"columns.motus" | trans}}</th>
        <th id="col-details">
          {{"columns.details" | trans}}
        </th>
      </tr>
    </thead>
    <tbody></tbody>
  </table>
</div>