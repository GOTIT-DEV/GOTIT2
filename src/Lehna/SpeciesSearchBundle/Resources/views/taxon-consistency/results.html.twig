{# Taxon consistency query results #}

{% trans_default_domain 'queries' %}

<div class="table-responsive-md">
	<table id="result-table" class="table table-condensed table-hover table-striped" style="width:100%">
		<thead>
			<tr class="table-header-meta">
				<th class="control"></th>
				<th colspan="3" class="border-right">
					{{"entity.biomaterial.label" | trans}}
				</th>
				<th colspan="4" class="border-right">
					{{"entity.individu.label" | trans}}
				</th>
				<th colspan="3">
					{{"entity.seq.label" | trans}}
				</th>
			</tr>
			<tr>
				<th class="control"></th>
				<th>
					{{"entity.biomaterial.code" | trans}}
				</th>
				<th>{{"columns.taxon" | trans}}</th>
				<th>{{"columns.critere" | trans}}</th>
				<th>
					{{"entity.individu.code.biomol" | trans}}
				</th>
				<th>
					{{"entity.individu.code.trimorpho" | trans}}
				</th>
				<th>{{"columns.taxon" | trans}}</th>
				<th>{{"columns.critere" | trans}}</th>
				<th>
					{{"entity.seq.code" | trans}}
				</th>
				<th>{{"columns.taxon" | trans}}</th>
				<th>{{"columns.critere" | trans}}</th>
			</tr>
		</thead>
		<tbody></tbody>
	</table>
</div>
