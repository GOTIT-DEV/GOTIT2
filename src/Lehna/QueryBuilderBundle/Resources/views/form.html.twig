<form id="main-form" action="#">
	<legend>
		<h2 style='margin-bottom:18px'>
			Query fields
		</h2>
	</legend>
</div>
<div class="row">
	<div class="col-xs-12 col-md-5 col-lg-6" style="padding-left:25px">
		<div class="panel panel-default">
			<div class="panel-body">
				<div class="row">
					<div class="col-xs-12 col-sm-7">
						<div class="form-group">
							<label class="control-label" for="niveau">
								Tables
							</label>
							<select id="get-data" >
    						</select>
						</div>
					</div>
					<div class="col-xs-12 col-sm-5">
						<div class="form-horizontal">
							<div class="form-group">
								<div class="checkbox checkbox-inline">
									<label class="control-label" title data-toggle="tooltip" data-placement="right">
										<input type="checkbox" value="2" data-toggle="collapse" id="getdata" class="collapsed" aria-expanded="false" data-target="#collapseConstraints">
										<strong>
											Contraintes
										</strong>
										<i class="fas fa-spinner fa-spin pull-right hidden" id="method-spinner" style="font-size:16px"></i>
									</label>
								</div>
							</div>
						</div>
					</div>

				</div>
			</div>
		</div>
	</div>
	<div class="panel-collapse collapse col-xs-11" id="collapseConstraints" aria-expanded="false">
		<div class="col-xs-12 col-sm-5 col-sm col-lg-3" style="padding-left:20px">
			<div class="panel panel-default">
				<div class="panel-heading">
					<strong>
						Choix des champs
					</strong>
				</div>
				<div class="panel-body">
					<div class="form-group">
						<div  id="show-data">
						</div>
					</div>
				</div>
			</div>
		</div>
		<div class="col-xs-2 col-sm-7">
			<div id="builder-basic"></div>
		</div>
	</div>




	<div class="addContraints" id="addContraints">
	</div>



	<div class="col-xs-12 btn-group pull-left group-actions">
		<button class="btn btn-xs btn-basic collapsed" type="button" onclick="addDiv()">
			<i class="glyphicon glyphicon-plus-sign" style="font-size:25px"></i>
		</button>
	</div>


	<template>	
	<div class="col-xs-12 col-sm-10" style="padding-left:25px">
		<div class="panel panel-default ">
			<div class="panel-heading">
				<strong>Nouvelle sélection</strong>
			</div>
			<div class="panel-body">
				<div class="row">
					<div class="col-xs-12 col-sm-4">
						<div class="form-group">
							<label for="lot-materiel" class="control-label">
								Table précédente</label>
							<select id="get-data2" ></select>
						</div>
					</div>
					<div class="col-xs-12 col-sm-2">
						<div class="form-group">
							<label for="individu" class="control-label">
								Choix jointure
							</label>
							<select id="get-data3" ></select>
						</div>
						<!-- <div class="btn-group bootstrap-select form-control concordance"></div> -->
					</div>
					<div class="col-xs-12 col-sm-4">
						<div class="form-group">
							<label for="sequence" class="control-label">
								Table adjacente
							</label>
							<select id="get-data4" ></select>
						</div>
					</div>
					<div class="col-xs-12 col-sm-2">
						<div class="form-horizontal">
							<div class="form-group">
								<div class="checkbox checkbox-inline">
									<label class="control-label" title data-toggle="tooltip" data-placement="right">
										<input type="checkbox" value="2" data-toggle="collapse" id="getconstraints"  class="collapsed" aria-expanded="false" data-target="#collapseConstraints2">
										<strong>
											Contraintes
										</strong>
										<i class="fas fa-spinner fa-spin pull-right hidden" id="method-spinner" style="font-size:16px"></i>
									</label>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
			<div class="panel-collapse collapse col-xs-12" id="collapseConstraints2" aria-expanded="false">
				<div class="col-xs-12 col-sm-5 col-sm col-lg-4" style="padding-left:20px">
					<div class="panel panel-default">
						<div class="panel-heading">
							<strong>
								Choix des champs
							</strong>
						</div>
						<div class="panel-body">
							<div class="form-group">
								<div  id="show-data2">
								</div>
							</div>
						</div>
					</div>
				</div>
				<div class="col-xs-2 col-sm-8">
					<div id="builder-basic-2">
					</div>
				</div>
			</div>
		</div>
	</template>
	</div>
	<div class="col-xs-12 col-sm-10" style="padding-left:25px">
		<div>
			<center>
				<button type="submit" onclick="getTable()" href="#resultats" value="Rechercher" class="btn btn-primary btn-lg" id="search-button" style="min-width: 10em" data-loading-text="<i class='fas fa-spinner fa-spin'></i>">
					{% trans %}
					Search
					{% endtrans %}
				</button>
			</center>
		</div>
	</div>
</div>
</form>
